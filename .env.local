# ============================================================================
# AI Provider Configuration
# ============================================================================

# AI Provider Selection
# Options: 'gemini', 'openai', 'anthropic', 'custom'
VITE_AI_PROVIDER=gemini

# AI API Key (use the appropriate key for your provider)
# Gemini: Get from https://makersuite.google.com/app/apikey
# OpenAI: Get from https://platform.openai.com/api-keys
# Anthropic: Get from https://console.anthropic.com/settings/keys
VITE_AI_API_KEY=AIzaSyBtuvDOZnf_2voNO0QObWMbtdBXLa9bDNo
#VITE_GEMINI_API_KEY=
# VITE_OPENAI_API_KEY=your_openai_key_here
# VITE_ANTHROPIC_API_KEY=your_anthropic_key_here

# Model Selection
# Gemini: gemini-2.5-flash, gemini-2.5-pro, gemini-2.0-flash-exp
# OpenAI: gpt-4o, gpt-4o-mini, gpt-4-turbo
# Anthropic: claude-3-5-sonnet-20241022, claude-3-5-haiku-20241022
VITE_AI_MODEL=gemini-2.5-flash

# Custom Provider Base URL (for self-hosted or custom endpoints)
# VITE_AI_BASE_URL=https://your-custom-api.com

# AI Generation Parameters
VITE_AI_TEMPERATURE=0.1
VITE_AI_MAX_TOKENS=4096

# ============================================================================
# Feature Flags
# ============================================================================

# Semantic Caching (default: true)
VITE_FEATURE_CACHE=false

# File Processing (default: true)
VITE_FEATURE_FILE_PROCESSING=true

# Future Features (default: false)
# VITE_FEATURE_COMPLIANCE=true
# VITE_FEATURE_SAFETY=true
# VITE_FEATURE_PRICING=true

# ============================================================================
# Rate Limiting
# ============================================================================

VITE_RATE_LIMIT_MAX_RETRIES=3
VITE_RATE_LIMIT_DELAY_MS=1000
VITE_RATE_LIMIT_EXPONENTIAL_BACKOFF=true

# ============================================================================
# File Processing
# ============================================================================

VITE_FILE_MAX_SIZE=10485760
VITE_FILE_SUPPORTED_FORMATS=pdf,png,jpg,jpeg,dwg
VITE_FILE_PDF_DPI=300

# ---------------------------------------------------------------------------
# Dev server / HMR overrides (used by vite.config.ts)
# ---------------------------------------------------------------------------
# Vite dev server port (overrides default 3000)
VITE_PORT=3000
# HMR client port (useful when tunneling or using a proxy)
VITE_HMR_CLIENT_PORT=3000
# Controls allowed hosts handling (true or comma-separated hosts)
VITE_ALLOWED_HOSTS=true
# Enable CORS for dev server (true/false)
VITE_CORS=true

